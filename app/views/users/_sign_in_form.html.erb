<div class="hidden signin_form">
  <%= form_for :user, url: sessions_path, id: 'login_form' do |f| %>
    <%= f.text_field :user_name, placeholder: 'Enter your username or email' %>
    <%= f.password_field :password, placeholder: 'Password' %>
    <%= f.submit "Sign In", :class=> "button", id: "login" %>
  <% end %>
</div>